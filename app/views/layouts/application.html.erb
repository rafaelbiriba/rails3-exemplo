<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag "screen", :media => 'screen' %>
  <%= stylesheet_link_tag "print", :media => 'print' %>
  <%= stylesheet_link_tag "custom", :media => 'screen' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="container">
    <%= render 'layouts/header' %>
    <section class="round">
      <%= yield %>
    </section>
    <%= render 'layouts/footer' %>
  </div>
</body>
</html>
